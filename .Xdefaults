st.foreground: #000000
st.background: #ffffff
st.font: Dina:size=10

! Colors
*background: #000000
*foreground: #B2B2B2
! black
*color0:  #000000
*color8:  #686868
! red
*color1:  #B21818
*color9:  #FF5454
! green
*color2:  #18B218
*color10: #54FF54
! yellow
*color3:  #B26818
*color11: #FFFF54
! blue
*color4:  #1818B2
*color12: #5454FF
! purple
*color5:  #B218B2
*color13: #FF54FF
! cyan
*color6:  #18B2B2
*color14: #54FFFF
! white
*color7:  #B2B2B2
*color15: #FFFFFF

URxvt.cursorColor: #ff00e0
URxvt.preeditType: OverTheSpot
URxvt.font: xft:Dina:size=10:antialias=false, xft:TerminessTTF Nerd Font Mono:size=10:antialias=false
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.urgentOnBell: true
URxvt.scrollBar: false
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true

! Extensions
URxvt.perl-ext-common: default,selection-to-clipboard,matcher,keyboard-select
URxvt.matcher.button: 1
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.underlineURLs: true
URxvt.colorUL: #4682B4
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true
URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease

! Scroll up
URxvt.keysym.M-k: command:\033]720;1\007
URxvt.keysym.M-j: command:\033]721;1\007
