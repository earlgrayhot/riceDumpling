URxvt.cursorColor: #ff00e0
URxvt.preeditType: OverTheSpot
URxvt.font: xft:Dina:size=10:antialias=false, xft:TerminessTTF Nerd Font Mono:size=10:antialias=false
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.urgentOnBell: true
URxvt.scrollBar: false
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true

! Extensions
URxvt.perl-ext-common: default,selection-to-clipboard,matcher,keyboard-select
URxvt.matcher.button: 1
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.underlineURLs: true
URxvt.colorUL: #4682B4
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true
URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease

! Scroll up
URxvt.keysym.M-k: command:\033]720;1\007
URxvt.keysym.M-j: command:\033]721;1\007

XTerm*faceName: Misc Tamsyn:antialias=false:autohint=false
XTerm*faceSize: 11
XTerm*translations: #override \n\
    Ctrl <Key> minus: smaller-vt-font() \n\
    Ctrl <Key> plus: larger-vt-font() \n\
    Ctrl <Key> 0: set-vt-font(d) \n\
    Ctrl Shift <Key> C: copy-selection(CLIPBOARD) \n\
    Ctrl Shift <Key> V: insert-selection(CLIPBOARD) \n\
XTerm*backarrowKey: false
XTerm.ttyModes: erase ^?
XTerm*ptyInitialErase: true
XTerm*decTerminalID: vt340
XTerm*numColorRegisters: 256
XTerm*bellIsUrgent: true
XTerm*selectToClipboard: true
XTerm*trimSelection: true
XTerm.termName: st-256color
